kCG;AACV,SAAK,cAAL;AACH;AApCU,CAAf","sourcesContent":["<template lang=\"\">\r\n    <div>\r\n        <div class=\"container-fluid\">\r\n            <div class=\"card\">\r\n                <div class=\"card-header\">\r\n                    New Blog\r\n                </div>\r\n                <div class=\"card-body\">\r\n                    <div class=\"row\" v-if=\"typeof data_post != 'undefined' \" >\r\n                        <div \r\n                            class=\"col-4 mb-2\" \r\n                            v-for=\"(data, index) in data_post.data\"\r\n                            v-bind:key=\"index\"\r\n                        >\r\n                            <div class=\"card\">\r\n                                <div class=\"card-header\">\r\n                                    Header\r\n                                </div>\r\n                                <div class=\"card-body\">\r\n                                    <h4 class=\"card-title\">{{ data.title }}</h4>\r\n                                    <p class=\"card-text\">{{ data.content }}</p>\r\n                                </div>\r\n                                <div class=\"card-footer text-muted\">\r\n                                    Footer\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                    <div v-if=\"typeof data_post != undefined\">\r\n                        <PaginatorDefault v-model=\"currentPage\" :dataPaginator=\"data_post\"/>\r\n                    </div>\r\n                </div>\r\n                <div class=\"card-footer text-muted\">\r\n                    \r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import BaseRequest from '../core/BaseRequest'\r\n    import PaginatorDefault from '../components/Paginator/PaginatorDefault.vue'  \r\n    export default {\r\n        name : 'Blog',\r\n        components :{\r\n            PaginatorDefault\r\n        },\r\n        data () {\r\n            return {\r\n                api : 'post?page=',\r\n                data_post : undefined,\r\n                errors: {},\r\n                currentPage: 1\r\n            }\r\n        },\r\n        methods : {\r\n            getDataPostAPI(){\r\n                BaseRequest.get(this.api + this.currentPage)\r\n                    .then(response => {\r\n                        // handle success\r\n                        this.data_post = response.data;\r\n                    })\r\n                    .catch(function(error) {\r\n                        // handle error\r\n                        this.errors = error.response.data;\r\n                    })\r\n                    .then(function () {\r\n                        // always executed\r\n                    });\r\n            }\r\n        },\r\n        watch : {\r\n            currentPage : function(){\r\n                this.getDataPostAPI();\r\n            }\r\n        },\r\n        beforeMount() {\r\n            this.getDataPostAPI()\r\n        },\r\n    }\r\n</script>\r\n\r\n<style lang=\"\">\r\n    \r\n</style>"],"sourceRoot":""}]}